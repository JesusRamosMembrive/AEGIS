            <description>return sum of two numbers</description>
            <parameters>
              <param name="num1" type="int, double, float" />
              <param name="num2" type="int, double, float" />
            </parameters>
            <returns type="float" />
          </method>
          <method name="subtract" visibility="public" static="true">
            <description>return subtract of two numbers</description>
            <parameters>
              <param name="param1" type="int, double or float" />
              <param name="param2" type="int, double or float" />
            </parameters>
            <returns type="float" />
          </method>
        </methods>
      </class>
    </classes>
  </module>
